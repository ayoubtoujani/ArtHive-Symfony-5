{% extends 'feed.html.twig' %}

{% block title %}Reclamationgroupe index{% endblock %}

{% block content %}
    <h1>Reclamationgroupe index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>IdReclamation</th>
                <th>DescReclamation</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for reclamationgroupe in reclamationgroupes %}
            <tr>
                <td>{{ reclamationgroupe.idReclamation }}</td>
                <td>{{ reclamationgroupe.descReclamation }}</td>
                <td>
                    <a href="{{ path('app_reclamationgroupe_show', {'idReclamation': reclamationgroupe.idReclamation}) }}">show</a>
                    <a href="{{ path('app_reclamationgroupe_edit', {'idReclamation': reclamationgroupe.idReclamation}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

{% endblock %}
